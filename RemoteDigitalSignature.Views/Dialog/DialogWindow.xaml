<Window x:Class="RemoteDigitalSignature.Views.Dialog.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:behaviors="clr-namespace:Common.Mvvm.Behaviors;assembly=Common.Mvvm"
        xmlns:viewModels="clr-namespace:RemoteDigitalSignature.ViewModels;assembly=RemoteDigitalSignature.ViewModels"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance viewModels:BaseViewModel}"
        Title="{Binding Path=Title, UpdateSourceTrigger=PropertyChanged}" Height="600" Width="1200"
        WindowState="Normal" 
        WindowStartupLocation="CenterOwner" 
        Icon="/KimberleyProcessCertification;component/favicon.ico" 
        ShowInTaskbar="False"
        behaviors:DialogCloser.DialogResult="{Binding Path=DialogResult, UpdateSourceTrigger=PropertyChanged}"/>
